nt":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"56"},{"filePath":"106","messages":"107","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"56"},{"filePath":"108","messages":"109","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"56"},"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/index.js",[],["110","111"],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/serviceWorker.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/App.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/NavBar/NavBar.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/Home/Home.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/Register/Register.jsx",["112"],"import React, {useRef} from \"react\";\nimport {Grid, TextField, Button } from \"@material-ui/core\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport LinkM from '@material-ui/core/Link';\nimport {Register} from \"../../actions/authActions\";\nimport {useDispatch} from \"react-redux\";\nimport swal from 'sweetalert';\nimport { withStyles } from \"@material-ui/core/styles\";\n\nconst ColoredInput = withStyles({\n  root: {\n    \"& label\": {\n        color: \"#FFF\",\n    },\n    \"& label.Mui-focused\": {\n      color: \"yellow\"\n    },\n    \"& .MuiInput-underline:after\": {\n      borderBottomColor: \"yellow\"\n    },\n    \"& .MuiOutlinedInput-root\": {\n      \"& fieldset\": {\n        borderColor: \"#FFF\"\n      },\n      \"&:hover fieldset\": {\n        borderColor: \"orange\",\n      },\n      \"&.Mui-focused fieldset\": {\n        borderColor: \"yellow\"\n      }\n    }\n  }\n})(TextField);\n\nexport default function RegisterComponent(props){\n    const classes = props.styles;\n    const history = useHistory();\n    const refEmail = useRef();\n    const refPassword = useRef();\n    const refName= useRef();\n    const refLastName= useRef();\n    const dispatch = useDispatch();\n    \n    const Registeruser = async(event) => {\n      event.preventDefault();\n      let name=`${refName.current.value} ${refLastName.current.value}`\n      //console.log(name)\n      //console.log(refEmail.current.value, refPassword.current.value)\n      if(refEmail.current.value=== \"\" ){\n        swal(\"¡Para registrarte!\", \"¡Ingresa tu dirección de correo!\", \"warning\");\n      }\n      if(refPassword.current.value=== \"\" ){\n        swal(\"¡Para registrarte!\", \"¡Ingresa tu contraseña!\", \"warning\");\n      }\n      if(refName.current.value=== \"\" || refLastName.current.value=== \"\"  ){\n        swal(\"¡Para registrarte!\", \"¡Ingresa tu nombre y apellido!\", \"warning\");\n      }\n      else{\n      try{\n        await dispatch(Register(`${refEmail.current.value}`, `${refPassword.current.value}`,`${refName.current.value}`, `${refLastName.current.value}`));\n        swal(\"¡Binvenido Al Cotorreo!\", \"¡Ya estás registrado! Inicia sesión\", \"success\");\n        history.push(\"/\");\n      }catch(error){\n        swal(\"¡Esta cuenta ya existe!\", \"¡La dirección de correo ya está registrada!\", \"error\");\n      }\n    }\n  }\n    return (\n        <form className={props.styles.form} noValidate>\n          <Grid container spacing={2}>\n            <Grid item xs={12} sm={6}>\n              <ColoredInput\n                variant=\"outlined\"\n                required\n                fullWidth\n                id=\"firstName\"\n                label=\"Nombre\"\n                name=\"firstName\"\n                autoComplete=\"fname\"\n                autoFocus\n                inputRef={refName}\n              />\n            </Grid>\n            <Grid item xs={12} sm={6}>\n              <ColoredInput\n                variant=\"outlined\"\n                required\n                fullWidth\n                id=\"lastName\"\n                label=\"Apellido\"\n                name=\"lastName\"\n                autoComplete=\"lname\"\n                inputRef={refLastName}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <ColoredInput\n                variant=\"outlined\"\n                required\n                fullWidth\n                id=\"email\"\n                label=\"Correo Electronico\"\n                name=\"email\"\n                autoComplete=\"email\"\n                inputRef={refEmail}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <ColoredInput\n                variant=\"outlined\"\n                required\n                fullWidth\n                name=\"password\"\n                label=\"Contraseña\"\n                type=\"password\"\n                id=\"password\"\n                autoComplete=\"current-password\"\n                inputRef={refPassword}\n              />\n            </Grid>\n          \n          </Grid>\n          <Button\n            type=\"submit\"\n            fullWidth\n            variant=\"contained\"\n            color=\"inherit\"\n            className={classes.submit}\n            onClick={(event) => Registeruser(event)}\n          >\n            Registraté\n          </Button>\n          <Grid container justify=\"flex-end\">\n            <Grid item>\n              <LinkM variant=\"body2\">\n              <Link to=\"/\" className={classes.text}>{\"¡Ya tienes cuenta! Inicia Sesión\"}</Link>\n              </LinkM>\n            </Grid>\n          </Grid>\n        </form>\n    )\n}","/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/LogIn/LogIn.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/ProtectedComponent/ProtectedComponent.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/Sidebar/Sidebar.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/Chat/Chat.js",["113"],"import React, { useState, useEffect } from \"react\";\nimport { Avatar, IconButton } from \"@material-ui/core\";\nimport {\n  AttachFile,\n  InsertEmoticon,\n  MoreVert,\n  SearchOutlined,\n} from \"@material-ui/icons\";\nimport \"./Chat.css\";\nimport MicIcon from \"@material-ui/icons/Mic\";\n\nconst Chat = (props) => {\n  const [input, setInput] = useState(\"\");\n\n  const sendMessage = async (e) => {\n    e.preventDefault();\n\n    setInput(\"\");\n  };\n\n  if (input){\n        let response = await fetch(\n          `https://academlo-whats.herokuapp.com/api/v1/messages`,\n          {\n            method: \"POST\",\n            headers: {\n              \"Content-type\": \"application/json\",\n            },\n            body: JSON.stringify({\n              userId: props.id,\n              conversationId: props.conversation._id,\n              message: input,\n              timestamp:\"\",\n              received: false\n                //\"https://upload.wikimedia.org/wikipedia/commons/f/f4/User_Avatar_2.png\",\n            }),\n          }\n        );\n        let results = await response.json();\n        setInput(\"\");\n        return(results);\n        //console.log(results);\n\n      }\n\n    useEffect(()=>{\n  \n      const getMessages = async(idToSearch) => {\n        try {\n          const res = await fetch(`https://academlo-whats.herokuapp.com/api/v1/conversations/${idToSearch}/messages`)\n          const response = await res.json();\n          console.log(response);\n          const messages=await response[0].messages;\n          setMessages(messages);\n        } \n        catch (error) {\n          console.log(error)\n        }\n      }\n      getMessages(props.conversation._id);\n  \n     //setConversations(getConversations(props.id));\n    },[props.conversation]);\n  \n  useEffect(()=>{\n    \n    const getMessages = async(idToSearch) => {\n      try {\n        const res = await fetch(`https://academlo-whats.herokuapp.com/api/v1/conversations/${idToSearch}/messages`)\n        const response = await res.json();\n        console.log(response);\n        const messages=await response[0].messages;\n        setMessages(messages);\n      } \n      catch (error) {\n        console.log(error)\n      }\n    }\n    getMessages(props.conversation._id);\n    \n   //setConversations(getConversations(props.id));\n  },[props.conversation]);\n\n  return (\n    \n    <div className=\"chat\">\n      <div className=\"chat__header\">\n        <Avatar src={`${props.conversation.membersObj[0].photoUrl}`}/>\n        <div className=\"chat__headerInfo\">\n          <h3>{props.conversation.membersObj[0].username}</h3>\n          <p>Visto por ultima vez a las... </p>\n        </div>\n        <div className=\"chat__headerRight\">\n          <IconButton>\n            <SearchOutlined />\n          </IconButton>\n          <IconButton>\n            <AttachFile />\n          </IconButton>\n          <IconButton>\n            <MoreVert />\n          </IconButton>\n        </div>\n      </div>\n\n      <div className=\"chat__body\">\n        {\n        messages.map((message, i) => {\n          return (\n            <p\n              key={i}\n              className={`chat__message ${\n                message.received && \"chat__reciever\"\n              }`}\n            >\n              <span className=\"chat__name\">{message.userId===props.id?props.conversation.membersObj[1].username:props.conversation.membersObj[0].username}</span>\n              {message.message}\n              {/* <span className=\"chat__timestamp\">{message.timestamp}</span> */}\n            </p>\n          );\n        })\n        }\n      </div>\n\n      <div className=\"chat__footer\">\n        <InsertEmoticon />\n        <form onSubmit={sendMessage}>\n          <input\n            className=\"chat__input\"\n\n            value={input}\n            onChange={(e) => {\n              setInput(e.target.value);\n            }}\n            type=\"text\"\n            placeholder=\"Escribe un mensaje\"\n          />\n          <button onClick={sendMessage} type=\"submit\">\n            Enviar\n          </button>\n        </form>\n        <MicIcon />\n      </div>\n    </div>\n    \n  )\n\n};\n\nexport default Chat;\n","/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/Denied/Denied.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/SidebarChat/SidebarChat.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/actions/authActions.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/firebase.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/containers/LoginContainer.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/store.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/reducers/authReducer.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/reducers/conversationReducer.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/containers/RegisterContainer.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/Forgotpassword.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/components/menuProfile.jsx",[],["114","115"],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/containers/ForgotpasswordContainer.jsx",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/conversationsDucks.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 2/React SVC/El-Cotorreo/src/usersDucks.js",[],"/Users/moonita/Documents/FullSatck/React Módulo 